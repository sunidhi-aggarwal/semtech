name: CI Pipeline (Using Reusable Workflow)

on:
  push:
    branches:
      - 'feature/**'
      - 'main'

jobs:
  call-reusable:
    uses: ./.github/workflows/java-build.yml
    with:
      java-version: '17'
      project-key: 'sun-org-semtech'
      sonar-org: 'sun-org'
      image-name: 'sunidhidocker/semtech-poc'
